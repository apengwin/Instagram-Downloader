{
  "manifest_version": 2,
  "name": "Instagram Downloader",
  "description": "Open in New Tab on Instagram",
  "version": "0.1",
  "permissions": [
    "https://instagram.com/",
    "notifications",
    "contextMenus"
  ],
  "background": {
    "scripts": ["script.js"] 
  },
  "content_scripts":[
    {
      "matches": ["https://*/*"],
      "js":["render.js","jquery.min.js", "onscreen.js"]
    }
  ]
}
